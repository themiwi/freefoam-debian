FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
    top             
    {
        type            fixedValue;
        value           uniform 1.25853;
    }

    inlet           
    {
        type            fixedValue;
        value           uniform 1;
    }

    outlet          
    {
        type            zeroGradient;
    }

    bottom          
    {
        type            symmetryPlane;
    }

    frontAndBack    
    {
        type            empty;
    }
}

