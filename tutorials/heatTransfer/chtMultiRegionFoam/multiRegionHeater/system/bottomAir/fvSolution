    p_rgh
    {
        solver           GAMG;
        tolerance        1e-7;
        relTol           0.01;

        smoother         GaussSeidel;

        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator     faceAreaPair;
        mergeLevels      1;
    }

    p_rghFinal
    {
        solver           GAMG;
        tolerance        1e-7;
        relTol           0;

        smoother         GaussSeidel;

        cacheAgglomeration true;
