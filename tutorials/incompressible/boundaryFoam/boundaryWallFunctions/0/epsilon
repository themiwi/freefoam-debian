FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-08;

boundaryField
{
    lowerWall
    {
        type            epsilonWallFunction;
        value           uniform 1e-08;
    }
    upperWall
    {
        type            epsilonWallFunction;
        value           uniform 1e-08;
    }
    frontBack
    {
        type            cyclic;
        value           uniform 1e-08;
    }
    defaultFaces
    {
        type            empty;
    }
}

